{
	"name": "ssc-website",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev --host 127.0.0.1",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"format": "prettier --write . --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
		"postbuild": "pagefind --site .vercel/output/static",
		"pagefind:local": "pnpm build && cp -r .vercel/output/static/pagefind public/pagefind"
	},
	"dependencies": {
		"@astro-community/astro-embed-youtube": "^0.4.3",
		"@astrojs/check": "^0.3.1",
		"@astrojs/markdoc": "^0.7.1",
		"@astrojs/node": "^6.0.3",
		"@astrojs/prefetch": "^0.4.1",
		"@astrojs/react": "^3.0.4",
		"@astrojs/sitemap": "^3.0.3",
		"@astrojs/tailwind": "^5.0.2",
		"@astrojs/vercel": "^5.2.0",
		"@keystatic/astro": "^2.0.3",
		"@keystatic/core": "^0.2.9",
		"@nanostores/react": "^0.7.1",
		"@octokit/core": "^5.0.2",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-separator": "^1.0.3",
		"@radix-ui/react-slot": "^1.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"astro": "^3.5.3",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"cmdk": "^0.2.0",
		"get-youtube-id": "^1.0.1",
		"iron-webcrypto": "^1.0.0",
		"lite-youtube-embed": "^0.3.0",
		"lucide-react": "^0.292.0",
		"magic-regexp": "^0.7.0",
		"nanostores": "^0.9.5",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-lite-youtube-embed": "^2.4.0",
		"sharp": "^0.32.6",
		"solid-js": "^1.4.3",
		"tailwind-merge": "^2.0.0",
		"tailwindcss": "^3.0.24",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.2.2"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"pagefind": "^1.0.3",
		"prettier": "^3.0.3",
		"prettier-plugin-astro": "^0.12.1",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"vercel": "^33.0.1"
	},
	"prettier": {
		"singleQuote": true,
		"singleAttributePerLine": true,
		"semi": false,
		"useTabs": true,
		"tabWidth": 2,
		"plugins": [
			"prettier-plugin-astro",
			"prettier-plugin-tailwindcss"
		]
	},
	"volta": {
		"node": "20.10.0"
	}
}
